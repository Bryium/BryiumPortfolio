{
  "version": 2,
  "builds": [
    {
      "src": "BryiumPortfolio/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "BryiumPortfolio/wsgi.pyy"
    }
  ],
  "functions": {
    "api/**.py": {
      "runtime": "python3.10.13"
    }
  }
}
