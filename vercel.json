{
  "version": 2,
  "functions": {
    "BryiumPortfolio/wsgi.py": {
      "runtime": "@vercel/python",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "BryiumPortfolio/wsgi.py"
    }
  ]
}
