{
  "version": 2,
  "builds": [
    {
      "src": "BryiumPortfolio/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "BryiumPortfolio/wsgi.py"
    }
  ],
  "functions": {
    "api/**.py": {
      "maxDuration": 10
    }
  }
}
